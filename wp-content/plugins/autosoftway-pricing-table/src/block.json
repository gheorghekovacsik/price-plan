{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/autosoftway-pricing-table",
	"version": "0.1.0",
	"title": "AutoSoftway Pricing Table",
	"category": "widgets",
	"description": "The AutoSoftway pricing table organizes the data in to table with support for USD and CAD prices",
	"example": {},
	"attributes": {
		"tiers": {
			"type": "array",
			"default": []
		},
		"featureCategories": {
			"type": "array",
			"default": []
		},
		"currency": {
			"type": "string",
			"default": "USD"
		}
	},
	"supports": {
		"color": {
			"background": false,
			"text": true,
			"button": true
		},
		"html": false,
		"typography": {
			"fontSize": true
		}
	},
	"textdomain": "autosoftway-pricing-table",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
